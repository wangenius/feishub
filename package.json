{
  "name": "feishub",
  "version": "1.0.15",
  "description": "A SDK for feishu platform",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "build:example": "rollup -c rollup.example.config.cjs",
    "example": "npm run build:example && node dist/example.cjs",
    "test:table": "npm run build:example && node dist/example.cjs",
    "example:dev": "ts-node src/example.ts"
  },
  "keywords": [
    "feishu",
    "table",
    "sdk"
  ],
  "author": "wangenius@qq.com",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "rollup": "^4.6.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.8.1",
    "typescript": "^5.3.2",
    "vitepress": "^1.0.0-rc.31"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangenius/feishub.git"
  },
  "bugs": {
    "url": "https://github.com/wangenius/feishub/issues"
  },
  "homepage": "https://github.com/wangenius/feishub#readme",
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  }
}
